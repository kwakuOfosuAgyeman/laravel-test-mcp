# Laravel Test MCP Configuration

# Test framework to use: 'pest', 'phpunit', or leave empty for auto-detection
TEST_MCP_FRAMEWORK=

# Test execution timeout in seconds (default: 300)
TEST_MCP_TIMEOUT=300

# Coverage driver to use: 'xdebug', 'pcov', or leave empty for auto-detection
TEST_MCP_COVERAGE_DRIVER=

# Rate Limiting Configuration
# Enable or disable rate limiting (default: true)
TEST_MCP_RATE_LIMIT_ENABLED=true

# Maximum requests per time window (default: 60)
TEST_MCP_RATE_LIMIT_MAX_ATTEMPTS=60

# Time window in seconds (default: 60)
TEST_MCP_RATE_LIMIT_DECAY_SECONDS=60
